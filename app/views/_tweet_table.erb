<!-- Partial generated from _tweet_table.erb -->
<h2><%= @username %>'s recent tweets:</h2>
<table>
  <% @tweets.each_with_index do |tweet, index| %>
  <tr>
    <td id="number"><%= index + 1 %></td>
    <td id="tweet"><%= tweet.text %><br><div id="time"><%= tweet.twitter_time %><div></td>
  </tr>
  <% end %>
</table>
<!-- end of _tweet_table.erb partial -->
